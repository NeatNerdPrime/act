{
  "name": "nuxt-ui-pro-template-saas",
  "private": true,
  "type": "module",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "better-sqlite3",
      "esbuild",
      "sharp",
      "simple-git-hooks",
      "unrs-resolver",
      "vue-demi"
    ]
  },
  "trustedDependencies": [
    "simple-git-hooks",
    "@tailwindcss/oxide"
  ],
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:log": "nuxt dev 2>&1 | tee logs/dev-$(date +%Y%m%d-%H%M%S).log",
    "dev:clean": "nuxt dev --clean-cache",
    "dev:clean-log": "nuxt dev --clean-cache 2>&1 | tee logs/dev-clean-$(date +%Y%m%d-%H%M%S).log",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "typecheck": "nuxt typecheck",
    "logs:clear": "rm -rf logs/*.log || true",
    "logs:dir": "mkdir -p logs",
    "prepare": "simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "pre-push": "pnpm typecheck"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ],
    "*.{md,yml,yaml,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.45",
    "@iconify-json/simple-icons": "^1.2.36",
